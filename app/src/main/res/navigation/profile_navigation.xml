<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/profile_navigation"
    app:startDestination="@id/profileDetailedFragment">
    <fragment
        android:id="@+id/profileDetailedFragment"
        android:name="com.example.hwaa.fragment.main.profile.ProfileDetailedFragment"
        android:label="fragment_profile_detailed"
        tools:layout="@layout/fragment_profile_detailed" >
        <action
            android:id="@+id/action_profileDetailedFragment_to_settingFragment"
            app:destination="@id/settingFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingFragment"
        android:name="com.example.hwaa.fragment.main.profile.SettingFragment"
        android:label="fragment_setting"
        tools:layout="@layout/fragment_setting" />
</navigation>